    "def count_rows_per_fdate_stream_nodedup(permno: int, align=\"quarter_end\", names=NAMES) -> pd.DataFrame:\n",
    "    counts = defaultdict(int)  # fdate -> 原始行数（不去重）\n",
    "    for name in names:\n",
    "        path = _resolve_json_path(name)\n",
    "        with open(path, \"rb\") as f:\n",
    "            for obj in ijson.items(f, \"item\"):\n",
    "                if obj.get(\"permno\") != permno:\n",
    "                    continue\n",
    "                dt = _parse_any_datetime(obj.get(\"fdate\"))\n",
    "                if pd.isna(dt):\n",
    "                    continue\n",
    "                fd = _align_to_quarter(dt, align=align)\n",
    "                counts[fd] += 1\n",
    "    return (pd.DataFrame({\"fdate\": list(counts.keys()), \"n_rows\": list(counts.values())})\n",
    "              .sort_values(\"fdate\").reset_index(drop=True))\n",
    "\n",
    "def total_rows_used_over_series_nodedup(permno: int, align=\"quarter_end\", names=NAMES) -> int:\n",
    "    per_date = count_rows_per_fdate_stream_nodedup(permno, align=align, names=names)\n",
    "    return int(per_date[\"n_rows\"].sum())\n",
    "\n",
    "\n",
    "def find_full_coverage_permnos_sorted_nodedup(align=\"quarter_end\", names=NAMES) -> pd.DataFrame:\n",
    "    all_fdates = set()\n",
    "    cover = defaultdict(set)  # permno -> {fdate}（覆盖到哪些日期）\n",
    "    counts = Counter()        # permno -> 原始行数（不去重）\n",
    "\n",
    "    # 一趟扫描\n",
    "    for name in names:\n",
    "        path = _resolve_json_path(name)\n",
    "        with open(path, \"rb\") as f:\n",
    "            for obj in ijson.items(f, \"item\"):\n",
    "                p = obj.get(\"permno\")\n",
    "                if p is None:\n",
    "                    continue\n",
    "                dt = _parse_any_datetime(obj.get(\"fdate\"))\n",
    "                if pd.isna(dt):\n",
    "                    continue\n",
    "                fd = _align_to_quarter(dt, align=align)\n",
    "                all_fdates.add(fd)\n",
    "                cover[p].add(fd)\n",
    "                counts[p] += 1\n",
    "\n",
    "    need = len(all_fdates)\n",
    "    full_permnos = [p for p, fs in cover.items() if len(fs) == need]\n",
    "    df = pd.DataFrame({\n",
    "        \"permno\": full_permnos,\n",
    "        \"n_rows\": [counts[p] for p in full_permnos],\n",
    "        \"n_fdates\": [len(cover[p]) for p in full_permnos],\n",
    "    })\n",
    "    return df.sort_values([\"n_rows\", \"permno\"]).reset_index(drop=True)"
   ]
  },
