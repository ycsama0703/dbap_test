  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "46b358cc",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "\n",
    "# 你已有的工具函数\n",
    "# _resolve_json_path(name) -> Path\n",
    "# _parse_any_datetime(x)   -> pd.Timestamp | NaT\n",
    "# _align_to_quarter(ts, align=\"quarter_end\") -> pd.Timestamp\n",
    "\n",
    "NAMES = (\"banks\",\"households\",\"insurance_companies\",\n",
    "         \"investment_advisors\",\"mutual_funds\",\"other\",\"pension_funds\")\n",
    "\n",
    "class _PbarReader:\n",
    "    \"\"\"包装文件对象，读取时同步更新一个或多个 tqdm 进度条（以字节计）\"\"\"\n",
    "    def __init__(self, f, pbars=()):\n",
    "        self.f = f\n",
    "        self.pbars = tuple(p for p in pbars if p is not None)\n",
    "    def read(self, n=-1):\n",
    "        chunk = self.f.read(n)\n",
    "        nbytes = len(chunk)\n",
    "        for p in self.pbars:\n",
    "            p.update(nbytes)\n",
    "        return chunk\n",
    "    # 部分解析器会调用 readline；一并支持\n",
    "    def readline(self, *args, **kwargs):\n",
    "        line = self.f.readline(*args, **kwargs)\n",
    "        nbytes = len(line)\n",
    "        for p in self.pbars:\n",
    "            p.update(nbytes)\n",
    "        return line\n",
    "\n",
    "def count_rows_per_fdate_stream(permno: int,\n",
    "                                align: str = \"quarter_end\",\n",
    "                                names=NAMES,\n",
    "                                progress: bool = True) -> pd.DataFrame:\n",
    "    \"\"\"\n",
    "    单趟流式扫描所有 JSON（顶层数组），统计给定 permno 在每个 fdate\n",
    "    需要用到多少条原始记录（以 (fdate, permno, mgrno) 去重）。\n",
    "    progress=True 时显示总进度与单文件进度。\n",
    "    \"\"\"\n",
    "    # 预计算总字节数用于总体进度\n",
    "    total_bytes = 0\n",
    "    file_sizes = {}\n",
    "    for name in names:\n",
    "        path = _resolve_json_path(name)\n",
    "        sz = os.path.getsize(path)\n",
    "        file_sizes[name] = sz\n",
    "        total_bytes += sz\n",
    "\n",
    "    bydate: dict[pd.Timestamp, set] = {}\n",
    "\n",
    "    total_pbar = tqdm(total=total_bytes, unit='B', unit_scale=True,\n",
    "                      unit_divisor=1024, desc='Scanning all files',\n",
    "                      disable=not progress)\n",
    "    try:\n",
    "        for name in names:\n",
    "            path = _resolve_json_path(name)\n",
    "            file_pbar = tqdm(total=file_sizes[name], unit='B', unit_scale=True,\n",
    "                             unit_divisor=1024, desc=f'{name}', leave=False,\n",
    "                             disable=not progress)\n",
    "            try:\n",
    "                with open(path, \"rb\") as f:\n",
    "                    reader = _PbarReader(f, pbars=(file_pbar, total_pbar))\n",
    "                    # 顶层数组的每个元素\n",
    "                    for obj in ijson.items(reader, \"item\"):\n",
    "                        if obj.get(\"permno\") != permno:\n",
    "                            continue\n",
    "                        dt = _parse_any_datetime(obj.get(\"fdate\"))\n",
    "                        if pd.isna(dt):\n",
    "                            continue\n",
    "                        fdate_aligned = _align_to_quarter(dt, align=align)\n",
    "                        mgrno = obj.get(\"mgrno\")\n",
    "                        bydate.setdefault(fdate_aligned, set()).add(mgrno)\n",
    "            finally:\n",
    "                file_pbar.close()\n",
    "    finally:\n",
    "        total_pbar.close()\n",
    "\n",
    "    out = pd.DataFrame({\n",
    "        \"fdate\": list(bydate.keys()),\n",
    "        \"n_rows\": [len(s) for s in bydate.values()],\n",
    "    }).sort_values(\"fdate\").reset_index(drop=True)\n",
    "    return out\n",
    "\n",
    "\n"
   ]
