    "# —— 小工具：对齐到季度起/末 —— \n",
    "def _align_to_quarter(ts: pd.Timestamp, align: str = \"quarter_end\") -> pd.Timestamp:\n",
    "    side = \"end\" if align == \"quarter_end\" else \"start\"\n",
    "    return ts.to_period(\"Q\").asfreq(\"Q\", side).to_timestamp()\n",
    "\n",
    "# —— 小工具：把任意形式的日期（字符串/秒/毫秒时间戳）转成 Timestamp —— \n",
    "def _parse_any_datetime(x):\n",
    "    s = pd.to_numeric(pd.Series([x]), errors=\"coerce\").iloc[0]\n",
    "    if pd.notna(s):\n",
    "        unit = \"ms\" if s > 1e11 else \"s\"\n",
    "        return pd.to_datetime(s, unit=unit, errors=\"coerce\")\n",
    "    return pd.to_datetime(x, errors=\"coerce\")\n",
    "\n",
    "def get_agg_df_stream(\n",
    "    permno,\n",
    "    fdata,\n",
    "    align: str = \"quarter_end\",\n",
    "    names = (\"banks\",\"households\",\"insurance_companies\",\n",
    "             \"investment_advisors\",\"mutual_funds\",\"other\",\"pension_funds\"),\n",
    "    flush_every: int = 200_000,\n",
    "):\n",
    "    target_dt = _align_to_quarter(pd.to_datetime(fdata), align=align)\n",
    "\n",
    "    frames = []\n",
    "    buf = []\n",
    "\n",
    "    for name in names:\n",
    "        path = _resolve_json_path(name)\n",
    "        with open(path, \"rb\") as f:\n",
    "            for obj in ijson.items(f, \"item\"):\n",
    "                if obj.get(\"permno\") != permno:\n",
    "                    continue\n",
    "                dt = _parse_any_datetime(obj.get(\"fdate\"))\n",
    "                if pd.isna(dt):\n",
    "                    continue\n",
    "                if _align_to_quarter(dt, align=align) != target_dt:\n",
    "                    continue\n",
    "                obj[\"fdate\"] = target_dt\n",
    "                buf.append(obj)\n",
    "\n",
    "                if len(buf) >= flush_every:\n",
    "                    part = pd.DataFrame(buf); buf.clear()\n",
    "                    part[\"target_q\"]    = part[\"fdate\"].dt.to_period(\"Q\")\n",
    "                    part[\"tplus1_q\"]    = part[\"target_q\"] + 1\n",
    "                    side = \"end\" if align == \"quarter_end\" else \"start\"\n",
    "                    # 关键修复：用 .dt.to_timestamp\n",
    "                    part[\"tplus1_date\"] = part[\"tplus1_q\"].dt.to_timestamp(how=side)\n",
    "                    frames.append(part)\n",
    "\n",
    "    if buf:\n",
    "        part = pd.DataFrame(buf)\n",
    "        if not part.empty:\n",
    "            part[\"target_q\"]    = part[\"fdate\"].dt.to_period(\"Q\")\n",
    "            part[\"tplus1_q\"]    = part[\"target_q\"] + 1\n",
    "            side = \"end\" if align == \"quarter_end\" else \"start\"\n",
    "            # 同样的修复\n",
    "            part[\"tplus1_date\"] = part[\"tplus1_q\"].dt.to_timestamp(how=side)\n",
    "            frames.append(part)\n",
    "\n",
    "    if not frames:\n",
    "        return pd.DataFrame()\n",
    "\n",
    "    df = pd.concat(frames, ignore_index=True)\n",
    "    key_cols = [c for c in [\"permno\", \"mgrno\", \"fdate\"] if c in df.columns]\n",
    "    if key_cols:\n",
    "        df = (df.sort_values(key_cols)\n",
    "                .drop_duplicates(subset=key_cols, keep=\"first\")\n",
    "                .reset_index(drop=True))\n",
    "    return df\n",
    "\n",
    "# (1) 枚举 permno 在所有文件中的所有 fdate（对齐后去重 & 排序）\n",
    "def list_all_fdates_for_permno(permno: int, align: str = \"quarter_end\", names=NAMES):\n",
    "    fdates = set()\n",
    "    for name in names:\n",
    "        path = _resolve_json_path(name)\n",
    "        with open(path, \"rb\") as f:\n",
    "            for obj in ijson.items(f, \"item\"):  # 顶层数组的每个元素\n",
    "                if obj.get(\"permno\") != permno:\n",
    "                    continue\n",
    "                dt = _parse_any_datetime(obj.get(\"fdate\"))\n",
    "                if pd.isna(dt):\n",
    "                    continue\n",
    "                fdates.add(_align_to_quarter(dt, align=align))\n",
    "    return sorted(fdates)\n",
    "\n",
    "def get_price_pred(permno, fdate, align=\"quarter_end\"):\n",
    "    df = get_agg_df_stream(permno, fdate, align=align)\n",
    "\n",
    "    holding_total = pd.to_numeric(df['holding'], errors='coerce').sum()\n",
    "    shares_total  = pd.to_numeric(df['shares'],  errors='coerce').sum()\n",
    "    pred_price = float(holding_total) / float(shares_total) if shares_total else float('nan')\n",
    "\n",
    "    prc_series = pd.to_numeric(df['prc'], errors='coerce').dropna()\n",
    "    true_price = float(prc_series.iat[0]) if not prc_series.empty else float('nan')\n",
    "\n",
    "    abs_err = abs(pred_price - true_price) if (not math.isnan(pred_price) and not math.isnan(true_price)) else float('nan')\n",
    "    return pred_price, true_price, abs_err\n",
    "\n",
    "def build_price_df_for_permno(permno: int, align: str = \"quarter_end\", names=NAMES):\n",
    "    all_fdates = list_all_fdates_for_permno(permno, align=align, names=names)\n",
    "    rows = []\n",
    "    for fdate in all_fdates:\n",
    "        pred, true, err = get_price_pred(permno, fdate, align=align)\n",
    "        rows.append({\n",
    "            \"permno\": permno,\n",
    "            \"fdate\":  fdate,          # 已按季度对齐的日期\n",
    "            \"pred_price\": float(pred) if pd.notna(pred) else float(\"nan\"),\n",
    "            \"true_price\": float(true) if pd.notna(true) else float(\"nan\"),\n",
    "            \"abs_err\":    float(err)  if pd.notna(err)  else float(\"nan\"),\n",
    "        })\n",
    "    return pd.DataFrame(rows).sort_values(\"fdate\").reset_index(drop=True)\n"
   ]
